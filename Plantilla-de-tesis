\midrule 
$[PdCl_4]^{2-}$ & 33.6 & 10.6 & 10.6 & 10.6  \\
\hline
$[PdF_4]^{2-}$ & 74.7 & 2.03 & 4.07 & 1.90 \\
\hline
$[Pd(OH)_4]^{2-}$ & 40.0 &	12.1 & 12.3 & 11.9 \\
\hline
$[Pd(CN)_4]^{2-}$ & 0.002 & 8.6 & 8.6	& 8.6 \\ 
\hline
$[PdH_4]^{2-}$ & 9.26 & 7.50 & 7.50 & 7.50 \\ 
\hline
$[Pd(H_2O)_4]^{2+}$ & 71.8 & 14.0 & 14.2 &	13.9 \\ 
\hline
$[Pd(NH_3)_4]^{2+}$ & 20.4 & 11.6 & 11.6 & 11.6 \\ 
\hline
$[Pd(PH_3)_4]^{2+}$ & 0.002 & 8.6 & 8.6 & 8.6 \\
\hline
$[Pd(CO)_4]^{2+}$ & 2.10 & 9.16 & 9.16 & 9.15 \\ 
